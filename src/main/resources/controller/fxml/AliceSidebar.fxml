<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>

<fx:root alignment="CENTER" spacing="20.0" type="VBox" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Circle fx:id="imageCircle" fill="DODGERBLUE" radius="100.0" stroke="BLACK" strokeType="INSIDE" />
      <Label text="alice">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="angerLabel" text="alice" textFill="#bfbfbf">
         <font>
            <Font name="System Italic" size="20.0" />
         </font>
      </Label>
   </children>
   <padding>
      <Insets bottom="50.0" top="50.0" />
   </padding>
</fx:root>
